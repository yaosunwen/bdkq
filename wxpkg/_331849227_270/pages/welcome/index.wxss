page {
    align-items: center;
    background: #fff;
    display: flex;
    flex-direction: column;
    height: 100vh;
    justify-content: center;
}

.img_1 {
    width: 100%;
}

.img_2 {
    width: 284rpx;
}

.error-panel {
    align-items: center;
    background: #fff;
    display: flex;
    flex-direction: column;
    height: 100vh;
    justify-content: center;
    left: 0;
    position: absolute;
    top: 0;
    width: 100vw;
    z-index: 99;
}

.error-panel .errorimg-box {
    width: 100%;
}

.error-panel .errorimg-box .errorimg {
    display: block;
    height: 245rpx;
    margin: 0 auto;
}

.error-panel .err-msg {
    color: #404040;
    font-size: 30rpx;
    margin: 60rpx 0;
}

.error-panel .reload {
    border: 1px solid var(--themeColor);
    border-radius: 15rpx;
    color: var(--themeColor);
    font-size: 30rpx;
    height: 76rpx;
    line-height: 76rpx;
    margin: 0 auto;
    text-align: center;
    width: 240rpx;
}

.error-panel .error-tip {
    bottom: 60rpx;
    color: #999;
    font-size: 28rpx;
    position: absolute;
    z-index: 999;
}

.load-box {
    align-items: center;
    background: rgba(0,0,0,.18);
    border-radius: 29rpx;
    color: #fff;
    display: flex;
    font-size: 24rpx;
    height: 58rpx;
    justify-content: center;
    line-height: 58rpx;
    position: absolute;
    right: 20rpx;
    text-align: left;
    top: 20rpx;
    width: 156rpx;
    z-index: 9;
}

.tui-loading-3 {
    animation: a 1s steps(12) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
    display: inline-block;
    height: 38rpx;
    vertical-align: middle;
    width: 38rpx;
}

@-webkit-keyframes a {
    0% {
        transform: rotate(0);
    }

    to {
        transform: rotate(1turn);
    }
}

@keyframes a {
    0% {
        transform: rotate(0);
    }

    to {
        transform: rotate(1turn);
    }
}

.l_tip {
    bottom: 48rpx;
    color: #999;
    display: inline-block;
    font-size: 24rpx;
    left: 50%;
    letter-spacing: 4rpx;
    position: absolute;
    text-align: center;
    transform: translate(-50%);
    width: 100%;
}